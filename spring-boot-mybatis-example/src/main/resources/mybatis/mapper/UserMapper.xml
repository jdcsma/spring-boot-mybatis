<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.UserMapper">
    <insert id="save" parameterType="jun.example.spring.boot.mybatis.domain.User" useGeneratedKeys="true">
        insert into tab_user(name,sex,age) values(#{name},#{sex},#{age})
    </insert>
</mapper>